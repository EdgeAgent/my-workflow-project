      "parameters": {
        "jsCode": "const output = $input.first().json.message?.content || $input.first().json.text;\nlet ideas;\n\ntry {\n  ideas = JSON.parse(output);\n} catch (e) {\n  // If JSON parsing fails, create default structure\n  ideas = [{\n    title: $input.first().json.content_topic,\n    hook: 'Trending topic',\n    audience: 'General',\n    viral_score: 7,\n    platforms: ['tiktok', 'twitter'],\n    talking_points: ['Point 1', 'Point 2']\n  }];\n}\n\nconst topIdea = ideas.sort((a, b) => b.viral_score - a.viral_score)[0];\n\nreturn {\n  json: {\n    selected_idea: topIdea,\n    all_ideas: ideas,\n    timestamp: new Date().toISOString()\n  }\n};"
      },
